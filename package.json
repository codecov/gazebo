{
  "name": "gazebo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^0.3.1",
    "@hookform/resolvers": "^1.3.0",
    "@sentry/react": "^6.2.0",
    "@stripe/react-stripe-js": "^1.3.0",
    "@stripe/stripe-js": "^1.13.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.17.0",
    "downshift": "^6.1.0",
    "elliptic": "^6.5.3",
    "immer": "^8.0.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "qs": "^6.9.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.4",
    "react-modal": "^3.12.1",
    "react-query": "^3.12.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-use": "^17.1.1",
    "web-vitals": "^1.1.0",
    "websocket-extensions": "^0.1.4",
    "y18n": "^5.0.5",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "craco start",
    "build": "export REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD); craco build",
    "test": "craco test",
    "sentry": "export REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD); craco build && node scripts/sentry.js",
    "test:ci": "craco test --collectCoverage --runInBand --reporters=default --reporters=jest-junit",
    "eject": "craco eject",
    "lint": "npx eslint --no-ignore --max-warnings=0 -c .eslintrc.json 'src/**/*.{js,jsx,ts,tsx,json}'",
    "bundlesize": "npx bundlesize",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "build-storybook -s public --no-dll",
    "chromatic": "npx chromatic --exit-zero-on-changes -ci",
    "format-staged": "npx prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "lint-staged": "eslint --fix --no-ignore --max-warnings=0 -c .eslintrc.json 'src/**/*.{js,jsx,ts,tsx,json}'",
    "postinstall": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "200 kB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "50 kB"
    }
  ],
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "@babel/core": "^7.13.1",
    "@sentry/cli": "^1.63.0",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/node-logger": "^6.1.20",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/react": "^6.1.20",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.7.3",
    "autoprefixer": "^9.8.6",
    "bundlesize": "^0.18.1",
    "eslint-config-prettier": "^8.1.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^5.1.2",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.4",
    "msw": "^0.27.0",
    "postcss": "^7.0.35",
    "postcss-font-magician": "2.3.1",
    "prettier": "^2.2.1",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3"
  }
}
