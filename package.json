    "@types/jest": "^29.5.1",
    "@types/js-cookie": "3.0.6",
    "@types/lodash": "4.17.6",
    "@types/node": "^20.5.7",
    "@types/prismjs": "^1.26.4",
    "@types/prop-types": "15.7.12",
    "@types/qs": "6.9.15",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.2.4",
    "@types/react-modal": "^3.16.2",
    "@types/react-router-dom": "^5.3.3",
