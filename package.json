{
  "name": "gazebo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:mutation": "npx stryker run",
    "test:ci": "craco test --collectCoverage --reporters=default --reporters=jest-junit",
    "eject": "craco eject",
    "lint": "eslint --fix --no-ignore --max-warnings=-1 -c .eslintrc.json 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "storybook dev -p 6006 -s public",
    "build-storybook": "storybook build -s public",
    "chromatic": "npx chromatic --exit-zero-on-changes -ci",
    "format-staged": "npx prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md}'",
    "postinstall": "husky install",
    "generate-icons": "node ./scripts/icons.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --no-ignore --max-warnings=0 -c .eslintrc.json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.5",
    "@radix-ui/react-popover": "^1.0.6",
    "@sentry/react": "^7.53.1",
    "@sentry/replay": "^7.53.1",
    "@sentry/tracing": "^7.53.1",
    "@stripe/react-stripe-js": "^1.7.2",
    "@stripe/stripe-js": "^1.52.1",
    "@tanstack/react-query": "^4.29.5",
    "@tanstack/react-table": "^8.9.1",
    "classnames": "^2.3.1",
    "d3-array": "^3.2.3",
    "d3-format": "^3.1.0",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-transition": "^3.0.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^1.3.4",
    "dompurify": "^2.3.8",
    "downshift": "^7.6.0",
    "js-cookie": "^3.0.5",
    "launchdarkly-react-client-sdk": "^2.25.1",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.3.1",
    "prop-types": "^15.8.1",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-day-picker": "^8.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.4.1",
    "react-modal": "^3.14.4",
    "react-router-dom": "^5.2.1",
    "react-router-dom-v5-compat": "^6.15.0",
    "react-use": "^17.2.4",
    "victory": "^36.6.11",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "@sentry/webpack-plugin": "^1.20.1",
    "@storybook/addon-a11y": "^7.4.0",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/manager-api": "^7.4.0",
    "@storybook/node-logger": "^7.4.0",
    "@storybook/preset-create-react-app": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-webpack5": "^7.4.0",
    "@storybook/theming": "^7.4.0",
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/jest-runner": "^5.6.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/eslint-plugin-query": "^4.29.4",
    "@tanstack/react-query-devtools": "^4.29.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "faker": "^5.5.3",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^7.0.4",
    "jest-junit": "^13.0.0",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "postcss": "^8.4.12",
    "prettier": "^2.8.8",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "remark-gfm": "^3.0.1",
    "source-map-explorer": "^2.5.3",
    "storybook": "^7.4.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "webpack": "^5.84.1"
  },
  "engines": {
    "node": "18.8.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!d3)/"
    ],
    "collectCoverageFrom": [
      "!src/**/*.stories.js",
      "!src/**/*.stories.jsx",
      "!src/**/*.stories.ts",
      "!src/**/*.stories.tsx"
    ],
    "globalSetup": "./src/setupTestGlobal.js"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "volta": {
    "node": "18.8.0",
    "npm": "8.19.1"
  },
  "overrides": {
    "react-refresh": "^0.12.0"
  }
}
