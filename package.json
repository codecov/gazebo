{
  "name": "gazebo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^2.8.5",
    "@reach/menu-button": "^0.16.2",
    "@sentry/react": "^6.13.2",
    "@stripe/react-stripe-js": "^1.7.2",
    "@stripe/stripe-js": "^1.25.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "dompurify": "^2.3.6",
    "downshift": "^6.1.7",
    "js-cookie": "^3.0.1",
    "launchdarkly-react-client-sdk": "^2.25.1",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.3.1",
    "prop-types": "^15.8.1",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-ace": "^10.1.0",
    "react-datepicker": "^4.7.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.1",
    "react-modal": "^3.14.4",
    "react-query": "3.39",
    "react-router-dom": "^5.2.1",
    "react-table": "^7.7.0",
    "react-use": "^17.2.4",
    "victory": "^36.3.1",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "export REACT_APP_SENTRY_RELEASE=\"$(git rev-parse --short HEAD)\" && react-scripts build",
    "test": "react-scripts test",
    "test:mutation": "npx stryker run",
    "sentry": "export REACT_APP_SENTRY_RELEASE=\"$(git rev-parse --short HEAD)\" && react-scripts build && node scripts/sentry.js",
    "test:ci": "react-scripts test --collectCoverage --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "lint": "npx eslint --fix --no-ignore --max-warnings=-1 -c .eslintrc.json 'src/**/*.{js,jsx,ts,tsx,json}'",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "build-storybook -s public --no-dll",
    "chromatic": "npx chromatic --exit-zero-on-changes -ci",
    "format-staged": "npx prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "postinstall": "husky install",
    "generate-icons": "node ./scripts/icons.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --no-ignore --max-warnings=0 -c .eslintrc.json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@sentry/cli": "^1.74.2",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/node-logger": "^6.4.20",
    "@storybook/preset-create-react-app": "^4.1.1",
    "@storybook/react": "^6.4.19",
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/jest-runner": "^5.6.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "faker": "^5.5.3",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^7.0.4",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.3.7",
    "msw": "^0.40.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^3.0.24",
    "webpack": "^5.70.0"
  },
  "engines": {
    "node": "16.14.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!d3)/"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
