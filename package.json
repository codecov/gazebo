    "prism-react-renderer": "^1.3.1",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "qs": "^6.11.1",
    "react": "^18.3.1",
    "react-ace": "^12.0.0",
    "react-day-picker": "^8.8.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "9.4.1",
    "react-modal": "^3.14.4",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.2.1",
    "react-router-dom-v5-compat": "^6.15.0",
    "react-use": "^17.2.4",
    "recharts": "^2.12.7",
    "semver": "7.5.2",
    "tailwind-merge": "^2.3.0",
    "victory": "^37.0.2",
    "zod": "^3.21.4"
  },
