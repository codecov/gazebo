{
  "name": "gazebo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^2.8.2",
    "@reach/menu-button": "^0.16.2",
    "@sentry/react": "^6.13.2",
    "@stripe/react-stripe-js": "^1.5.0",
    "@stripe/stripe-js": "^1.21.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "dompurify": "^2.3.3",
    "downshift": "^6.1.7",
    "js-cookie": "^3.0.1",
    "launchdarkly-react-client-sdk": "^2.23.2",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "prism-react-renderer": "^1.2.1",
    "prop-types": "^15.7.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-ace": "^9.4.4",
    "react-dates": "^21.8.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.20.5",
    "react-image": "^4.0.3",
    "react-modal": "^3.14.3",
    "react-query": "^3.24.5",
    "react-router-dom": "^5.2.1",
    "react-scripts": "^4.0.3",
    "react-table": "^7.7.0",
    "react-use": "^17.2.4",
    "victory": "^36.2.0",
    "web-vitals": "^2.1.2",
    "websocket-extensions": "^0.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "craco start",
    "build": "export REACT_APP_SENTRY_RELEASE=\"$(git rev-parse --short HEAD)\" && craco build",
    "test": "craco test",
    "sentry": "export REACT_APP_SENTRY_RELEASE=\"$(git rev-parse --short HEAD)\" && craco build && node scripts/sentry.js",
    "test:ci": "craco test --collectCoverage --reporters=default --reporters=jest-junit",
    "eject": "craco eject",
    "lint": "npx eslint --no-ignore --max-warnings=0 -c .eslintrc.json 'src/**/*.{js,jsx,ts,tsx,json}'",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "build-storybook -s public --no-dll",
    "chromatic": "npx chromatic --exit-zero-on-changes -ci",
    "format-staged": "npx prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
    "postinstall": "husky install",
    "generate-icons": "node ./scripts/icons.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix --no-ignore --max-warnings=0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "200 kB"
    },
    {
      "path": "./build/static/css/*.css",
      "maxSize": "50 kB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@craco/craco": "^6.4.1",
    "@sentry/cli": "^1.71.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.4.1",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^9.8.8",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^7.0.4",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.2",
    "msw": "^0.35.0",
    "postcss": "^7.0.36",
    "postcss-font-magician": "2.3.1",
    "prettier": "^2.4.1",
    "react-is": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!d3)/"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
